  },
  "homepage": "https://github.com/fer-nando-machado/node-transactions#readme",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "dependencies": {
    "bull": "^4.15.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "ioredis": "^5.4.1",
    "knex": "^3.1.0",
    "pg": "^8.12.0",
    "pg-promise": "^11.9.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
